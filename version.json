{
    "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
    "version": "0.1",
    "assemblyVersion": {
      "precision": "revision"
    },
    "nugetPackageVersion": {
      "semVer": 2
    },
    "publicReleaseRefSpec": [
      "^refs/heads/main$"
    ],
    "cloudBuild": {
      "setVersionVariables": true,
      "buildNumber": {
        "enabled": false,
        "includeCommitId": {
          "when": "nonPublicReleaseOnly",
          "where": "buildMetadata"
        }
      }
    }
  }